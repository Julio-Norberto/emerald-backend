{
	"compilerOptions": {
		"target": "ES2020",
		"lib": ["ES2020"],
		"module": "ES2020",
		"rootDir": "./",
		"moduleResolution": "Node",
		"outDir": "./build",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitAny": false,
		"skipLibCheck": true
	}
}